<Slider name="DrSpammerOpacitySlider" orientation="HORIZONTAL" minValue="0" maxValue="100" defaultValue="30" valueStep="10">
    <Size x="100" y="15"/>
    <Anchors>
        <Anchor point="BOTTOMRIGHT">
            <Offset x="-50" y="20"/>
        </Anchor>
    </Anchors>
    <Backdrop bgFile="Interface\Buttons\UI-SliderBar-Background" edgeFile="Interface\Buttons\UI-SliderBar-Border">
        <EdgeSize><AbsValue val="8"/></EdgeSize>
        <TileSize><AbsValue val="8"/></TileSize>
    </Backdrop>
    <ThumbTexture file="Interface\Buttons\UI-SliderBar-Button-Horizontal"/>
    <Scripts>
        <OnValueChanged>
            local value = self:GetValue()
            local alpha = value / 100
            DrSpammerSetting:SetBackdropColor(0.1, 0.1, 0.1, alpha)
            getglobal("DrSpammerOpacityText"):SetText("Прозрачность: " .. value .. "%")
            if DrSpammerDB then
                DrSpammerDB.Opacity = alpha
            end
        </OnValueChanged>
    </Scripts>
</Slider>

