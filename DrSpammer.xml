	<Frame name="DrSpammer" toplevel="true" enableMouse="true" movable="true" parent="UIParent" hidden="false">
		<Size x="100" y="20"/>
		<Anchors><Anchor point="CENTER"/></Anchors>
		<Layers>
			<Layer>
				<FontString name="DrSpammerText" inherits="GameFontHighlight" text="B_Spa |cffff0000выкл|r">
					<Size x="90" y="16"/>
					<Anchors><Anchor point="CENTER"/></Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Scripts>
			<OnUpdate>DrSpammer:OnUpdate()</OnUpdate>
			<OnMouseUp>
				if this.isMoving then
					this:StopMovingOrSizing()
					this.isMoving = false
					DrSpammer:SavePosition(1)
				end
				DrSpammer:OnMouseUp(self, arg1)
			</OnMouseUp>
			<OnMouseDown>
				if (not this.isLocked or this.isLocked == 0) and arg1 == "LeftButton" then
					this:StartMoving()
					this.isMoving = true
				end
				DrSpammer:OnMouseDown(self, arg1)
			</OnMouseDown>
			<OnEnter>DrSpammer:OnEnter()</OnEnter>
			<OnLeave>DrSpammer:OnLeave()</OnLeave>
		</Scripts>
	</Frame>
	
